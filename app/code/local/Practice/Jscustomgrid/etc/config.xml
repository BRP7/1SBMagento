<?xml version="1.0"?>
<config>
    <modules>
        <Practice_Jscustomgrid>
            <version>0.1.0</version>
        </Practice_Jscustomgrid>
    </modules>
    <global>
        <helpers>
            <practice_jscustomgrid>
                <class>Practice_Jscustomgrid_Helper</class>
            </practice_jscustomgrid>
        </helpers>
        <models>
            <practice_jscustomgrid>
                <class>Practice_Jscustomgrid_Model</class>
            </practice_jscustomgrid>
        </models>
    </global>
    <adminhtml>
        <menu>
            <practice_jscustomgrid translate="title" module="practice_jscustomgrid">
                <title>Jscustomgrid</title>
                <sort_order>100</sort_order>
                <children>
                    <product translate="title">
                        <title>Products</title>
                        <action>practice_jscustomgrid/adminhtml_product</action>
                    </product>
                </children>
            </practice_jscustomgrid>
        </menu>
        <acl>
            <resources>
                <all>
                    <title>Allow Everything</title>
                </all>
                <admin>
                    <children>
                        <practice_jscustomgrid translate="title" module="practice_jscustomgrid">
                            <title>Jscustomgrid</title>
                            <sort_order>100</sort_order>
                            <children>
                                <product translate="title">
                                    <title>Products</title>
                                </product>
                            </children>
                        </practice_jscustomgrid>
                    </children>
                </admin>
            </resources>
        </acl>
        <layout>
            <updates>
                <practice_jscustomgrid>
                    <file>practice_jscustomgrid.xml</file>
                    </practice_jscustomgrid>
                </updates>
            </layout>
    </adminhtml>
</config>
