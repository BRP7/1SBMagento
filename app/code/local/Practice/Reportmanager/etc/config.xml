<config>
    <modules>
        <Practice_Reportmanager>
            <version>0.1.1</version>
        </Practice_Reportmanager>
    </modules>
    <global>
        <events>
            <sales_order_item_save_before>
                <observers>
                    <repoer_sales_order_item_save_before_observer>
                        <class>practice_reportmanager/observer</class>
                        <method>customSalesOrderItemBeforeSave</method>
                    </repoer_sales_order_item_save_before_observer>
                </observers>
            </sales_order_item_save_before>
            <sales_order_item_save_after>
                <observers>
                    <repoer_sales_order_item_save_after_observer>
                        <class>practice_reportmanager/observer</class>
                        <method>customSalesOrderItemAfterSave</method>
                    </repoer_sales_order_item_save_after_observer>
                </observers>
            </sales_order_item_save_after>
            <order_cancel_after>
                <observers>
                    <repoer_sales_order_save_delete_observer>
                        <class>practice_reportmanager/observer</class>
                        <method>customSalesOrderBeforeDelete</method>
                    </repoer_sales_order_save_delete_observer>
                </observers>
            </order_cancel_after>
        </events>
        <models>
            <practice_reportmanager>
                <class>Practice_Reportmanager_Model</class>
                <resourceModel>practice_reportmanager_resource</resourceModel>
            </practice_reportmanager>
            <practice_reportmanager_resource>
                <class>Practice_Reportmanager_Model_Resource</class>
                <entities>
                    <reportmanager>
                        <table>ccc_filter_report</table>
                    </reportmanager>
                </entities>
            </practice_reportmanager_resource>
        </models>
        <resources>
            <practice_reportmanager_setup>
                <setup>
                    <module>Practice_Reportmanager</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </practice_reportmanager_setup>
        </resources>
    </global>
</config>
