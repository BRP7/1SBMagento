<?xml version="1.0"?>
<config>
    <modules>
        <Practice_Customgrid>
            <version>1.0.0</version>
        </Practice_Customgrid>
    </modules>
    <global>
        <resources>
            <customgrid_setup>
                <setup>
                    <module>Practice_Customgrid</module>
                    <class>Mage_Core_Model_Resource_Setup</class>
                </setup>
            </customgrid_setup>
        </resources>
    </global>
    <frontend>
        <routers>
            <customgrid>
                <use>standard</use>
                <args>
                    <module>Practice_Customgrid</module>
                    <frontName>customgrid</frontName>
                </args>
            </customgrid>
        </routers>
    </frontend>
</config>
